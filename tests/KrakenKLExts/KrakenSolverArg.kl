require Math;

struct KrakenSolverArg {
    String name;
    String connectionType;
    String dataType;
};


// Default Constructor
function KrakenSolverArg()
{

}

function KrakenSolverArg(
  String name,
  String connectionType,
  String dataType)
{
  this.name = name;
  this.connectionType = connectionType;
  this.dataType = dataType;
}