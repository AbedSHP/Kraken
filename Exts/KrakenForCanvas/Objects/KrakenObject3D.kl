require Math;
require Containers;
require Kraken;

struct KrakenObject3D {
  String name;
  String buildName;
  String path;
  String layer;
  String component;
  Xfo xfo;
  Boolean visibility;
  Color color;
  String flags[String];
};

/// \dfgPresetFolder Constraints
function Xfo KrakenObject3D.transform?(Xfo child) {
  return this.xfo * child;
}

function Xfo(KrakenObject3D k) {
  this = k.xfo;
}
